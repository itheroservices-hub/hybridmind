{
  "name": "hybridmind",
  "displayName": "HybridMind AI Assistant",
  "description": "Multi-model AI orchestration platform with 21 models from 8 providers. Run workflows, compare models, and leverage the best AI for each task.",
  "publisher": "hybridmind",
  "version": "1.3.1",
  "license": "MIT",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/itheroservices-hub/hybridmind.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Machine Learning",
    "Programming Languages",
    "Snippets"
  ],
  "keywords": [
    "ai",
    "artificial intelligence",
    "llm",
    "gpt",
    "claude",
    "gemini",
    "code assistant",
    "productivity"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "lint": "eslint src --ext ts",
    "package": "vsce package"
  },
  "activationEvents": [],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "hybridmind",
          "title": "HybridMind",
          "icon": "$(robot)"
        }
      ]
    },
    "views": {
      "hybridmind": [
        {
          "type": "webview",
          "id": "hybridmind.chatView",
          "name": "Chat",
          "icon": "$(comment-discussion)",
          "contextualTitle": "HybridMind Chat"
        }
      ]
    },
    "commands": [
      { "command": "hybridmind.openChat", "title": "HybridMind: Open Chat Window", "icon": "$(comment-discussion)" },
      { "command": "hybridmind.inlineChat", "title": "HybridMind: Start Inline Chat", "icon": "$(edit)" },
      { "command": "hybridmind.manageLicense", "title": "HybridMind: Manage License", "icon": "$(key)" },
      { "command": "hybridmind.quickChat", "title": "HybridMind: Quick Chat" },
      { "command": "hybridmind.explainCode", "title": "HybridMind: Explain Code" },
      { "command": "hybridmind.reviewCode", "title": "HybridMind: Review Code" },
      { "command": "hybridmind.optimizeCode", "title": "HybridMind: Optimize Code" },
      { "command": "hybridmind.generateTests", "title": "HybridMind: Generate Tests" },
      { "command": "hybridmind.fixBugs", "title": "HybridMind: Fix Bugs" }
    ],
    "keybindings": [
      {
        "command": "hybridmind.inlineChat",
        "key": "ctrl+k",
        "mac": "cmd+k",
        "when": "editorTextFocus"
      },
      {
        "command": "hybridmind.openChat",
        "key": "ctrl+shift+h",
        "mac": "cmd+shift+h"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "hybridmind.openChat",
          "when": "true"
        },
        {
          "command": "hybridmind.inlineChat",
          "when": "editorTextFocus"
        }
      ],
      "editor/context": [
        {
          "command": "hybridmind.inlineChat",
          "group": "hybridmind@0",
          "when": "editorTextFocus"
        },
        {
          "command": "hybridmind.explainCode",
          "group": "hybridmind@1"
        },
        {
          "command": "hybridmind.reviewCode",
          "group": "hybridmind@2"
        },
        {
          "command": "hybridmind.optimizeCode",
          "group": "hybridmind@3"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "HybridMind",
      "properties": {
        "hybridmind.licenseKey": {
          "type": "string",
          "default": "",
          "description": "HybridMind Pro license key. Get yours at https://hybridmind.dev/pricing"
        },
        "hybridmind.groqApiKey": {
          "type": "string",
          "default": "",
          "description": "Groq API Key (for Llama, Mixtral models). Get from: https://console.groq.com/keys"
        },
        "hybridmind.geminiApiKey": {
          "type": "string",
          "default": "",
          "description": "Google Gemini API Key (for Gemini models). Get from: https://aistudio.google.com/app/apikey"
        },
        "hybridmind.deepseekApiKey": {
          "type": "string",
          "default": "",
          "description": "DeepSeek API Key (for DeepSeek models). Get from: https://platform.deepseek.com/api_keys"
        },
        "hybridmind.qwenApiKey": {
          "type": "string",
          "default": "",
          "description": "Qwen API Key (for Qwen models). Get from: https://dashscope.console.aliyun.com/apiKey"
        },
        "hybridmind.openaiApiKey": {
          "type": "string",
          "default": "",
          "description": "OpenAI API Key (for GPT models). Get from: https://platform.openai.com/api-keys"
        },
        "hybridmind.anthropicApiKey": {
          "type": "string",
          "default": "",
          "description": "Anthropic API Key (for Claude models). Get from: https://console.anthropic.com/settings/keys"
        }
      }
    }
  }
}
